{
  "manifest_version": 3,
  "name": "Play Mini (vite version)!",
  "version": "1.2.0",
  "description": "Launch a random NY mini.",
  "permissions": ["tabs", "storage", "history"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Play!"
  },
  "web_accessible_resources": [
    {
      "resources": ["dates.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
