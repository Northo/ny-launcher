{
  "manifest_version": 3,
  "name": "Play Mini!",
  "version": "1.0.0",
  "description": "Launch a random NY mini.",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Play!"
  },
  "web_accessible_resources": [
    {
      "resources": ["dates.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
